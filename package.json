  "ava": {
    "files": [
      "**/*.test.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/helpers/setup-browser-env",
      "./test/helpers/setup-react-native-mock"
    ],
    "babel": "inherit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
